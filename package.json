{
  "name": "jest-repro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "lodash.find": "^4.3.0",
    "lodash.isequalwith": "^4.3.1",
    "lodash.merge": "^4.3.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "enzyme": "^2.2.0",
    "jest": "^15.0.2",
    "jest-cli": "^15.0.2",
    "lodash.values": "^4.1.0",
    "react-addons-test-utils": "^15.3.0"
  },
  "jest": {
    "automock": true,
    "verbose": true,
    "testRegex": [
      "/test/.*\\.js$"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/lodash"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/components/"
    ]
  }
}
